      PROCEDURE ListArticles()
      
      arrProduct is array of 8-byte int
      
      QRY_AUTHOR_ARTICLES.pAuthor = m_nUserAccountID
      QRY_AUTHOR_ARTICLES.pStatus = RPT_STATUS_ARTICLE_VALIDATED
