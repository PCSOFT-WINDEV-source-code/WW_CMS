  p_codes :
   -
     code : |1-
      LOCAL 
      	MyStream is a rssStream
      	MyChannel is a rssChannel
      	MyEntry is a rssEntry
      	
      	MyStream.Version = "2.0"
      
      	MyChannel..Title = ValeurSiteParameter(CMS_TITLE_RSS_STREAM)
      	MyChannel..Description = ValeurSiteParameter(CMS_DESCRIPTION_RSS_STREAM)
      	MyChannel..UpdateDate = DateSys() + TimeSys()
      
      	// Characteristics of the channel
      	arrProduct is array of 8-byte int = CArticle::s_ListRecentOnes(20)
      	clProduct is CArticle
      	FOR EACH nItemID of arrProduct
      		IF clProduct.bLoadInfo(nItemID) THEN
      		
      			// Characteristics of the entries of channel 1
      			MyEntry..Author = clProduct.AuthorName()
      			MyEntry..Description = clProduct.m_sArticleSummary
      			MyEntry..Title = clProduct.m_sArticleTitle
      			MyEntry..PublicationDate = clProduct.m_dtPublicationDate
      			MyEntry..Category.Valeur = clProduct.CategoryName()
      						
      			// Adds the entry to the channel
      			Add(MyChannel..Entry,MyEntry)
      			
      		END
      	
      	END	
      	// Adds the channel to the stream
      	Add(MyStream..Channel,MyChannel)
      
      
      rssDisplay(MyStream)
