  procedures :
   -
     name : _FillCategoryTreeView
     procedure_id : 1679900583273736117
     type_code : 14
     code : |1-
      // Summary: Fills a branch in the category treeview and calls recursively
      // Syntax:
      //_FillCategoryTreeView (<nParentID> is 8-byte int, <sCurrentPath> is string)
      //
      // Parameters:
      //	nParentID (8-byte integer): Identifier of the current branch
      //	sCurrentPath (ANSI string): <specify the role of sCurrentPath>
      // Return value:
      // 	None
      //
      PROCEDURE PRIVATE _FillCategoryTreeView( nParentID is 8-byte int, sCurrentPath is string )
      
      arrCategory is array of 8-byte int = CCategory::s_ListChildren(nParentID)
      clCateg is CCategory
      nCategoryID is 8-byte int
      FOR EACH nCategoryID of arrCategory 
      	// Add a category
      	IF clCateg.bLoadInfo(nCategoryID) THEN
      		sPath is string = sCurrentPath + [TAB] + clCateg.m_sCategoryCaption
      		
      		// ImagePath returns an absolute URL, you must supply a URL relative to TreeAdd
      		// therefore, calculate the relative URL
      		sImage is string = clCateg.ImagePath()
      		sRelativeImageURL is string = sImage
      		IF sImage<>"" THEN
      			sRelativeImageURL = sImage[[Length(SET_SiteUtility.gsSiteURLDatabase)+1 TO ]]
      		END
      
      		TreeAdd(TREE_Categories,sPath,(sImage<>"" ? sRelativeImageURL ELSE tvDefault),(sImage<>"" ? sRelativeImageURL ELSE tvDefault),nCategoryID,tvLast,clCateg.m_sCategoryDescription)
      		
      		
      		
      		aaParam is ASSOCIATIVE ARRAY of strings = [ [ "id", nCategoryID ] ]
      		IF gCallbackCategoryClick=Null THEN
      			TREE_Categories[sPath]..URL = BuildURL(SET_CMS.gsURLCategoryPage,aaParam)
      		END
      		
      		// Recursive call
      		_FillCategoryTreeView(nCategoryID,sPath)
      	END
      END
     type : 458752
   -
     name : FillCategoryTreeView
     procedure_id : 1679900583273801653
     type_code : 14
     code : |1+
      // Summary: <specify the action of the procedure>
      // Syntax:
      //FillCategoryTreeView (<nDatabaseID> is 8-byte int)
      //
      // Parameters:
      //	nDatabaseID (8-byte integer): <specify the role of nDatabaseID>
      // Return value:
      // 	None
      //
      PROCEDURE FillCategoryTreeView( nDatabaseID is 8-byte int )
      
      TreeDeleteAll(TREE_Categories)
      
      _FillCategoryTreeView(nDatabaseID,"")
      
      STC_Categories..Visible = (TREE_Categories..Occurrence>0)
      
     type : 458752
